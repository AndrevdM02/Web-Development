#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger-tools@0.9.16/node_modules/swagger-tools/bin/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger-tools@0.9.16/node_modules/swagger-tools/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger-tools@0.9.16/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger-tools@0.9.16/node_modules/swagger-tools/bin/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger-tools@0.9.16/node_modules/swagger-tools/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger-tools@0.9.16/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../swagger-tools/bin/swagger-tools" "$@"
else
  exec node  "$basedir/../swagger-tools/bin/swagger-tools" "$@"
fi
