#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/andre/RW334/group-22/backend/node_modules/.pnpm/json-refs@2.1.7/node_modules/json-refs/bin/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/json-refs@2.1.7/node_modules/json-refs/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/json-refs@2.1.7/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/andre/RW334/group-22/backend/node_modules/.pnpm/json-refs@2.1.7/node_modules/json-refs/bin/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/json-refs@2.1.7/node_modules/json-refs/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/json-refs@2.1.7/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/json-refs" "$@"
else
  exec node  "$basedir/../../bin/json-refs" "$@"
fi
