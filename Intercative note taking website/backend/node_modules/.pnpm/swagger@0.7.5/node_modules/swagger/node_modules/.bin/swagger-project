#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger@0.7.5/node_modules/swagger/bin/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger@0.7.5/node_modules/swagger/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger@0.7.5/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger@0.7.5/node_modules/swagger/bin/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger@0.7.5/node_modules/swagger/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/swagger@0.7.5/node_modules:/home/andre/RW334/group-22/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/swagger-project.js" "$@"
else
  exec node  "$basedir/../../bin/swagger-project.js" "$@"
fi
